}

if (csrfToken &amp;&amp; csrfToken.trim()) {
  vars.put(&quot;CSRF_TOKEN&quot;, csrfToken.trim())
  log.info(&quot;✓ 已注入 CSRF_TOKEN 变量&quot;)
} else {
  vars.put(&quot;CSRF_TOKEN&quot;, &quot;&quot;)
  log.info(&quot;未发现csrfToken，继续不带X-CSRF-Token&quot;)
}</stringProp>
