          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Extract Tokens and UserId to Properties" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper

String responseData = prev.getResponseDataAsString();
if (prev.isSuccessful() &amp;&amp; responseData) { // Only process if sampler was successful and response is not empty
    try {
        def slurper = new JsonSlurper();
        def jsonResponse = slurper.parseText(responseData);

        String userId = jsonResponse.userId?.toString();
        String userToken = jsonResponse.userToken?.toString();
        String userAuthorization = jsonResponse.userAuthorization?.toString();

        if (userId) {
            props.put(&quot;extracted_userId_for_logout&quot;, userId);
            log.info(&quot;JSR223: Set JMeter property extracted_userId_for_logout to: &quot; + userId);
        } else {
            log.warn(&quot;JSR223: userId not found in login response.&quot;);
        }
        if (userToken) {
            props.put(&quot;extracted_user_token&quot;, userToken);
            log.info(&quot;JSR223: Set JMeter property extracted_user_token.&quot;);
        } else {
            log.warn(&quot;JSR223: userToken not found in login response.&quot;);
        }
        if (userAuthorization) {
            props.put(&quot;extracted_user_authorization&quot;, userAuthorization);
            log.info(&quot;JSR223: Set JMeter property extracted_user_authorization.&quot;);
        } else {
            log.warn(&quot;JSR223: userAuthorization not found in login response.&quot;);
        }
    } catch (Exception e) {
        log.error(&quot;JSR223: Error parsing login JSON response: &quot; + e.getMessage());
    }
} else if (!prev.isSuccessful()) {
    log.info(&quot;JSR223: Login sampler failed, skipping property extraction.&quot;);
} else if (!responseData){
    log.warn(&quot;JSR223: Login response data is empty, skipping property extraction.&quot;);
}</stringProp>
