          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Extract Tokens to Properties and Write to CSV" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper
import java.io.File
// Note: Removed imports that are no longer needed for automatic path detection
// import org.apache.jmeter.services.FileServer
// import org.apache.jmeter.threads.JMeterContextService
// import org.apache.jmeter.threads.JMeterContext

// --- 1. Get response data ---
String responseData = prev.getResponseDataAsString()
if (!prev.isSuccessful() || !responseData) {
    log.warn(&quot;JSR223: Sampler failed or response was empty. Skipping token extraction and CSV writing.&quot;)
    return // Exit script
}

try {
    // --- 3. Write/Overwrite CSV file in the project root &apos;data&apos; folder ---
    // 获取项目根目录，优先用 -JprojectRoot 传递，否则用当前工作目录
    String projectRoot = props.get(&quot;projectRoot&quot;) ?: System.getProperty(&quot;user.dir&quot;)
    File dataDir = new File(projectRoot + File.separator + &quot;data&quot;)
    if (!dataDir.exists()) {
        dataDir.mkdirs()
    }
    File tokenFile = new File(dataDir, &quot;auth_tokens.csv&quot;)

    def slurper = new JsonSlurper()
    def jsonResponse = slurper.parseText(responseData)

    // --- 提取所有需要的字段 ---
    String userId = jsonResponse.userId?.toString()
    String userToken = jsonResponse.userToken?.toString()
    String userAuthorization = jsonResponse.userAuthorization?.toString()
    String userEmail = jsonResponse.userEmail?.toString()
    String userDisplayName = jsonResponse.userDisplayName?.toString()
    String accountId = jsonResponse.accountId?.toString()
    String userType = jsonResponse.userType?.toString()


    // --- 2. Set JMeter Properties for immediate use in this test plan ---
    if (userId) { props.put(&quot;extracted_userId&quot;, userId); log.info(&quot;JSR223: Set JMeter property &apos;extracted_userId&apos; to: &quot; + userId) }
    if (userToken) { props.put(&quot;extracted_user_token&quot;, userToken); log.info(&quot;JSR223: Set JMeter property &apos;extracted_user_token&apos;.&quot;) }
    if (userAuthorization) { props.put(&quot;extracted_user_authorization&quot;, userAuthorization); log.info(&quot;JSR223: Set JMeter property &apos;extracted_user_authorization&apos;.&quot;) }
    if (userEmail) { props.put(&quot;extracted_userEmail&quot;, userEmail); log.info(&quot;JSR223: Set JMeter property &apos;extracted_userEmail&apos; to: &quot; + userEmail) }
    if (userDisplayName) { props.put(&quot;extracted_userDisplayName&quot;, userDisplayName); log.info(&quot;JSR223: Set JMeter property &apos;extracted_userDisplayName&apos; to: &quot; + userDisplayName) }
    if (accountId) { props.put(&quot;extracted_accountId&quot;, accountId); log.info(&quot;JSR223: Set JMeter property &apos;extracted_accountId&apos; to: &quot; + accountId) }
    if (userType) { props.put(&quot;extracted_userType&quot;, userType); log.info(&quot;JSR223: Set JMeter property &apos;extracted_userType&apos; to: &quot; + userType) }


    // --- 3. Write/Overwrite CSV file in the project root &apos;data&apos; folder ---
    // 确保核心的认证信息存在
    if (userId &amp;&amp; userToken &amp;&amp; userAuthorization) {
        // 添加时间戳以确保文件每次都更新
        String timestamp = new Date().format(&quot;yyyy-MM-dd HH:mm:ss.SSS&quot;)

        // 使用 .withWriter 自动管理资源
        tokenFile.withWriter(&apos;UTF-8&apos;, { writer -&gt;
            // *** 新增：更新CSV文件的表头 ***
            writer.writeLine(&apos;userId,userToken,userAuthorization,userEmail,userDisplayName,accountId,userType,lastUpdated&apos;)
            // *** 新增：更新写入CSV文件的数据行 ***
            writer.writeLine(&quot;${userId},${userToken},${userAuthorization},${userEmail},${userDisplayName},${accountId},${userType},${timestamp}&quot;)
        })

        log.info(&quot;JSR223: Successfully wrote auth tokens to &quot; + tokenFile.absolutePath + &quot; at &quot; + timestamp)

    } else {
        log.error(&quot;JSR223: Missing one or more required tokens (userId, userToken, userAuthorization). CSV file was not written.&quot;)
    }

} catch (Exception e) {
    log.error(&quot;JSR223: Error processing JSON response or writing to CSV file.&quot;, e)
    prev.setSuccessful(false)
    prev.setResponseMessage(&quot;Error in JSR223 PostProcessor: &quot; + e.getMessage())
}
