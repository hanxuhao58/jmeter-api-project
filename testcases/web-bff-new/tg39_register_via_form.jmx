        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="注入风控Cookie">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import org.apache.jmeter.protocol.http.control.CookieManager
import org.apache.jmeter.protocol.http.control.Cookie

// 获取配置中的额外Cookie
def extraCookies = props.get(&quot;EXTRA_COOKIES&quot;)
log.info(&quot;=== 注入风控Cookie ===&quot;)
log.info(&quot;EXTRA_COOKIES配置: &quot; + (extraCookies ?: &quot;未配置&quot;))

// 获取CookieManager
def cookieManager = sampler.getCookieManager()
if (cookieManager != null &amp;&amp; extraCookies != null &amp;&amp; !extraCookies.isEmpty()) {
    // 解析并添加Cookie
    extraCookies.split(&quot;;&quot;).each { cookiePair -&gt;
        def parts = cookiePair.trim().split(&quot;=&quot;, 2)
        if (parts.length == 2) {
            def name = parts[0].trim()
            def value = parts[1].trim()
            
            // 创建Cookie对象
            def cookie = new Cookie(name, value, &quot;www.gumtree.com&quot;, &quot;/&quot;, false, 0)
            cookieManager.add(cookie)
            log.info(&quot;已添加Cookie: &quot; + name)
        }
    }
}

// 生成随机邮箱避免重复注册
def randomNum = new Random().nextInt(999999)
def timestamp = System.currentTimeMillis()
def testEmail = &quot;jmetertest${randomNum}@gmail.com&quot;
vars.put(&quot;TEST_EMAIL&quot;, testEmail)
log.info(&quot;测试邮箱: &quot; + testEmail)

log.info(&quot;=== Cookie注入完成 ===&quot;)
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223PreProcessor>
