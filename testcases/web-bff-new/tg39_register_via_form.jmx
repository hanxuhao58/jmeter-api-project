          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="记录响应详情" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">log.info(&quot;=== 注册接口响应分析 ===&quot;)
log.info(&quot;状态码: &quot; + prev.getResponseCode())
log.info(&quot;响应消息: &quot; + prev.getResponseMessage())
log.info(&quot;响应时间: &quot; + prev.getTime() + &quot;ms&quot;)

def responseCode = prev.getResponseCode()
def responseBody = prev.getResponseDataAsString()

// 分析响应
if (responseCode == &quot;200&quot;) {
    log.info(&quot;✓ 注册请求成功&quot;)
} else if (responseCode == &quot;247&quot;) {
    log.warn(&quot;⚠ WAF拦截 (247) - 需要更新风控Cookie&quot;)
    log.info(&quot;响应内容: &quot; + responseBody.take(500))
} else if (responseCode == &quot;400&quot;) {
    log.info(&quot;⚠ 请求参数错误 (400)&quot;)
    log.info(&quot;响应内容: &quot; + responseBody)
} else if (responseCode == &quot;403&quot;) {
    log.warn(&quot;⚠ 禁止访问 (403) - 可能是IP被封或Cookie无效&quot;)
} else if (responseCode == &quot;429&quot;) {
    log.warn(&quot;⚠ 请求过于频繁 (429) - 需要降低请求频率&quot;)
} else {
    log.info(&quot;响应码: &quot; + responseCode)
    log.info(&quot;响应内容: &quot; + responseBody.take(500))
}

log.info(&quot;=== 响应分析完成 ===&quot;)
</stringProp>
